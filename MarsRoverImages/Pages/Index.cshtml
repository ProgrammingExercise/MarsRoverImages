@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
    <div class="row">
        @foreach (var image in Model.ImageUrls)
        {
        <div class="col-md-4">
            <div class="thumbnail">
                <a href="@image">
                    <img src="@image" style="width:100%">
                </a>
            </div>
        </div>
        }
    </div>
